{
  "module": {
    "name": "feature",
    "type": "feature",
    "description": "$string:module_desc",
    "mainElement": "FeatureAbility",
    "deviceTypes": [
      "2in1",
      "default",
      "tablet"
    ],
    "deliveryWithInstall": true,
    "installationFree": false,
    "pages": "$profile:main_pages",
    "abilities": [
      {
        "name": "FeatureAbility",
