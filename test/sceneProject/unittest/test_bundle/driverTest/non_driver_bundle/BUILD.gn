ohos_hap("non_driver_entry_hap") {
  hap_profile = "entry/src/main/module.json"
  hap_name = "non_driver_entry_hap"
  subsystem_name = "bundlemanager"
  final_hap_path = "$root_out_dir/tests/unittest/bundle_framework/bundlemgrservice/resource/test_bundle/driver_test/${hap_name}.hap"
  testonly = true
  deps = [
    ":hjs_demo_js_assets",
    ":hjs_demo_resources",
  ]
  certificate_profile = "${bundle_framework_path}/test/sceneProject/signature/com.example.driverTest.p7b"
}

